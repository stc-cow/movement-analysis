{
  "buildCommand": "pnpm install && pnpm run build:client",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/(.*)$",
      "dest": "/index.html"
    }
  ],
  "note": "This project uses GitHub Pages and Express server. Vercel config kept for reference."
}
